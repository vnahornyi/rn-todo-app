{
  "name": "todoapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --config ./web/vite.config.ts",
    "build": "tsc -p ./web/tsconfig.json && vite build --config ./web/vite.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "npm run watchman-clean && react-native start",
    "test": "jest",
    "pod-install": "cd ios && pod install && cd ..",
    "gradlew-clean": "cd android && ./gradlew clean && cd ..",
    "watchman-clean": "watchman watch-del-all",
    "build-android": "npm run watchman-clean && cd android && ./gradlew assembleRelease && cd ..",
    "ios-release": "npm run watchman-clean && react-native run-ios --mode=Release",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile --typescript"
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^6.10.0",
    "@formatjs/intl-displaynames": "^6.5.0",
    "@formatjs/intl-getcanonicallocales": "^2.2.1",
    "@formatjs/intl-listformat": "^7.4.0",
    "@formatjs/intl-locale": "^3.3.2",
    "@formatjs/intl-numberformat": "^8.7.0",
    "@formatjs/intl-pluralrules": "^5.2.4",
    "@formatjs/intl-relativetimeformat": "^11.2.4",
    "@lingui/detect-locale": "^4.4.0",
    "@lingui/react": "^4.4.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/datetimepicker": "^7.4.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "date-fns": "^2.30.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-native": "0.72.3",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.8.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.23.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.10.0",
    "react-native-web": "^0.19.7",
    "react-native-webview": "^13.3.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@lingui/cli": "^4.4.0",
    "@lingui/macro": "^4.4.0",
    "@lingui/swc-plugin": "^4.0.4",
    "@lingui/vite-plugin": "^4.4.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "babel-jest": "^29.2.1",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "engines": {
    "node": ">=16"
  }
}
